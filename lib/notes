 testAuthEndpoints();  this is used in the main for testing

 void testAuthEndpoints() async {
   print('ğŸ” Testing auth endpoints...');

   final dio = Dio();
   final baseUrl = 'http://192.168.18.56:8080'; // Your working IP

   try {
     // Test Registration
     print('1. Testing registration...');
     final registerResponse = await dio.post(
       '$baseUrl/api/auth/register',
       data: {
         'username': 'testuser233',
         'password': 'testpass123'
       },
     );
     print('âœ… Registration response: ${registerResponse.data}');

     // Test Login
     print('2. Testing login...');
     final loginResponse = await dio.post(
       '$baseUrl/api/auth/login',
       data: {
         'username': 'testuser23',
         'password': 'testpass123'
       },
     );
     print('âœ… Login response: ${loginResponse.data}');

     // Extract JWT token
     final token = loginResponse.data['token'];
     print('ğŸ”‘ Token received: ${token != null ? "YES" : "NO"}');

     // Test authenticated endpoint with token
     if (token != null) {
       dio.options.headers['Authorization'] = 'Bearer $token';

       print('3. Testing authenticated endpoint...');
       final testResponse = await dio.get('$baseUrl/api/test/echo');
       print('âœ… Authenticated test: ${testResponse.data}');
     }

   } catch (e) {
     print('âŒ Error: $e');
     if (e is DioException) {
       print('Response: ${e.response?.data}');
     }
   }
 }

 above is the code for testing weather the api is working or not